---
export const prerender = false
import Project from "../components/es/Project.astro"
import Layout from "../layouts/es/Layout.astro"
import {findProjectBySlug} from '../lib/api/es/findProjectBySlug.js'
import { SITE_URL } from "../lib/constants"
const {projectSlug} = Astro.params 
const project = findProjectBySlug(projectSlug)
---

<Layout title=`Rafa Canosa | ${project.name}` description={project.description} canonical=`${SITE_URL}/${project.slug}`>
    <main>
        {project ? 
        <Project project={project}/> :
        null
        }
    </main>
</Layout>